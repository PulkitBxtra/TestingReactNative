    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/xtraBiteLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pulkitbatra.xtraBite"
    },
    "web": {
      "favicon": "./assets/favicon-32x32.png",
      "output": "server",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "2dabdbf1-e747-4df7-9013-d6966454e275"
      }
    }
  }
}
