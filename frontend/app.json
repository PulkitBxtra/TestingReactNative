{
  "expo": {
    "name": "xtraBite",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/xtraBiteLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/xtraBiteLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
